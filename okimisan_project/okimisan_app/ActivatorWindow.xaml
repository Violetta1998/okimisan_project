<Window x:Class="okimisan_app.ActivatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:okimisan_app"
        mc:Ignorable="d"
        Title="Activator" Height="150" Width="400" ResizeMode="NoResize" PreviewKeyDown="Window_PreviewKeyDown">
    <Grid>
        <Label x:Name="label" Content="Введите код активации" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Center" Height="23" Margin="0,46,0,0" TextWrapping="NoWrap" Text="" VerticalAlignment="Top" Width="200"/>
        <Button x:Name="button" Content="Принять" HorizontalAlignment="Center" Margin="0,80,0,0" VerticalAlignment="Top" Width="110" Click="button_Click"/>
    </Grid>
</Window>
